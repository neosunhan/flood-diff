{
    "name": "coogee-5-01norm",
    "phase": "train",
    "distributed": false,
    "amp": true,
    "latent": false,
    "dem": true,
    "path": {
        "log": "logs",
        "tb_logger": "tb_logger",
        "results": "results",
        "checkpoint": "checkpoint",
        "resume_state": "coogee-5-01norm/checkpoint/I100000_E86"
    },
    "datasets": {
        "meta": {
            "max_depth": 3200,
            "dem_max_value": 440,
            "dem_min_value": -20,
            "norm_min": 0,
            "norm_max": 1,
            "channels": 1,
            "image_size": 512
        },
        "train": {
            "catchment": "Coogee",
            "dataroot": "../data/coogee",
            "batch_size": 8,
            "num_workers": 1,
            "use_shuffle": true,
            "data_len": -1
        },
        "test": {
            "catchment": "Coogee 1k",
            "dataroot": "../data/coogee_1k",
            "batch_size": 40,
            "use_shuffle": false,
            "data_len": -1,
            "epochs": 10
        },
        "valid": {
            "catchment": "Coogee 1k",
            "dataroot": "../data/coogee_1k",
            "batch_size": 40,
            "use_shuffle": false,
            "data_len": -1,
            "epochs": 1
        }
    },
    "model": {
        "which_model_G": "sr3",
        "finetune_norm": false,
        "unet": {
            "in_channel": 3,
            "out_channel": 1,
            "inner_channel": 64,
            "norm_groups": 16,
            "channel_multiplier": [
                1,
                2,
                4,
                8,
                16
            ],
            "attn_res": [
                32
            ],
            "res_blocks": 3,
            "dropout": 0
        },
        "beta_schedule": {
            "train": {
                "schedule": "linear",
                "n_timestep": 1000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            },
            "test": {
                "schedule": "linear",
                "n_timestep": 1000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            }
        },
        "diffusion": {
            "image_size": 512,
            "loss_type": "l1",
            "conditional": true 
        }
    },
    "train": {
        "n_iter": 300000,
        "val_freq": 5e4,
        "save_checkpoint_freq": 5e4,
        "print_freq": 1000,
        "optimizer": {
            "type": "adam",
            "lr": 1e-5
        }
    },
    "test": {
        "save_results": false,
        "thresholds": [5]
    }
}