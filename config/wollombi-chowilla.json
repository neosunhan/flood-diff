{
    "name": "wollombi-chowilla",
    "phase": "train",
    "distributed": false,
    "amp": true,
    "latent": true,
    "path": {
        "log": "logs",
        "tb_logger": "tb_logger",
        "results": "results",
        "checkpoint": "checkpoint",
        "resume_state": "wollombi-chowilla/checkpoint/I201000_E1462",
        "vae_state": "../autoencoder/all_std/checkpoint/model_and_optimizer_epoch_5.pth",
        "vae_dem_state": "../autoencoder/all_dem_std/checkpoint/model_and_optimizer_epoch_2000.pth"
    },
    "datasets": {
        "meta": {
            "max_depth": 3200,
            "dem_max_value": 440,
            "dem_min_value": -20,
            "norm_min": -1,
            "norm_max": 1,
            "channels": 1,
            "image_size": 512
        },
        "train": {
            "catchment": "Chowilla Latent",
            "dataroot": "../autoencoder/all_std/latent/ep5/chowilla",
            "batch_size": 256,
            "num_workers": 1,
            "use_shuffle": true,
            "data_len": -1
        },
        "test": {
            "catchment": "Wollombi 1k",
            "dataroot": "../data/wollombi_1k",
            "batch_size": 40,
            "use_shuffle": false,
            "data_len": -1,
            "epochs": 1
        }
    },
    "model": {
        "which_model_G": "ddpm",
        "finetune_norm": false,
        "unet": {
            "in_channel": 12,
            "out_channel": 4,
            "inner_channel": 64,
            "norm_groups": 16,
            "channel_multiplier": [
                1,
                2,
                4,
                8,
                16
            ],
            "attn_res": [
                16
            ],
            "res_blocks": 1,
            "dropout": 0
        },
        "beta_schedule": {
            "train": {
                "schedule": "linear",
                "n_timestep": 1000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            },
            "test": {
                "schedule": "linear",
                "n_timestep": 1000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            }
        },
        "diffusion": {
            "image_size": 64,
            "loss_type": "l2",
            "conditional": true 
        }
    },
    "train": {
        "n_iter": 201000,
        "val_freq": 2e4,
        "save_checkpoint_freq": 200,
        "print_freq": 200,
        "optimizer": {
            "type": "adam",
            "lr": 1e-6,
            "lr_scheduler": {
                "schedule": "linear_warmup",
                "until": 10000
            }
        }
    },
    "test": {
        "save_results": false,
        "threshold": 5
    }
}